{
  "name": "odhiamboatieno/laravel-html",
  "description": "HTML and Form Builders for the Laravel Framework (Laravel 8â€“12 Compatible)",
  "license": "MIT",
  "homepage": "https://laravelcollective.com",
  "support": {
    "issues": "https://github.com/odhiamboatieno/laravel-html/issues",
    "source": "https://github.com/odhiamboatieno/laravel-html"
  },
  "authors": [
    {
      "name": "Adam Engebretson",
      "email": "adam@laravelcollective.com"
    },
    {
      "name": "Taylor Otwell",
      "email": "taylorotwell@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3 <8.4",
    "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/routing": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/session": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/view": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.5|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Collective\\Html\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "12.x-dev"
    },
    "laravel": {
      "providers": [
        "Collective\\Html\\HtmlServiceProvider"
      ],
      "aliases": {
        "Form": "Collective\\Html\\FormFacade",
        "Html": "Collective\\Html\\HtmlFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
